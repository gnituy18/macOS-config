#!/bin/bash

xcode-select --install

/usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"

brew tap caskroom/cask

brew install bash
brew install macvim
brew install git
brew install cmake
brew install ruby
brew install python
brew install python3
brew install node
brew install npm

brew cask install google-chrome

./setup.sh
